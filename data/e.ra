LOAD Student
LOAD Marks
FEMALE_STUDENTS <- SELECT Stud_sex == 1 FROM Student
ADVISOR_ONE <- SELECT Advisor_id == 1 FROM FEMALE_STUDENTS 
CROSS_TABLE <- CROSS ADVISOR_ONE, Marks
JOIN_TABLE <- SELECT ADVISOR_ONE_Stud_Id == Marks_Stud_Id FROM CROSS_TABLE
GOOD_MATH <- SELECT Maths_marks > 70 FROM JOIN_TABLE
FINAL_TABLE <- PROJECT ADVISOR_ONE_Stud_Id, Maths_marks FROM GOOD_MATH
RENAME ADVISOR_ONE_Stud_Id TO Stud_Id FROM FINAL_TABLE
PRINT FINAL_TABLE
