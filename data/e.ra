LOAD Student
LOAD Marks
FEMALE_STUDENTS <- SELECT Stud_sex == 1 FROM Student
ADVISOR_ONE <- SELECT Advisor_id == 1 FROM FEMALE_STUDENTS 
CROSS_FEMALES <- CROSS ADVISOR_ONE, Marks
JOIN_FEMALES <- SELECT ADVISOR_ONE_Stud_Id == Marks_Stud_Id FROM CROSS_FEMALES
GOOD_MATH <- SELECT Maths_marks > 70 FROM JOIN_FEMALES 
SELECTED_FEMALES <- PROJECT ADVISOR_ONE_Stud_Id, Maths_marks FROM GOOD_MATH
RENAME ADVISOR_ONE_Stud_Id TO Stud_Id FROM SELECTED_FEMALES 
PRINT SELECTED_FEMALES 
