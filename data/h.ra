LOAD Student
LOAD Marks 
H_CROSS_TABLE <- CROSS Student, Marks
H_JOIN_TABLE <- SELECT Student_Stud_Id == Marks_Stud_Id FROM H_CROSS_TABLE
H_ADVISOR_TABLE <- SELECT Advisor_id == 2 FROM H_JOIN_TABLE 
H_MARKS_TABLE <- SELECT Maths_marks > 90 FROM H_ADVISOR_TABLE
H_FINAL_TABLE <- PROJECT Student_Stud_Id FROM H_MARKS_TABLE
RENAME Student_Stud_Id TO Stud_Id FROM H_FINAL_TABLE
PRINT H_FINAL_TABLE
