LOAD Student
LOAD Marks 
CROSS_TABLE <- CROSS Student, Marks
JOIN_TABLE <- SELECT Student_Stud_Id == Marks_Stud_Id FROM CROSS_TABLE
ADVISOR_TABLE <- SELECT Advisor_id == 2 FROM JOIN_TABLE 
MARKS_TABLE <- SELECT Maths_marks > 90 FROM ADVISOR_TABLE
FINAL_TABLE <- PROJECT Student_Stud_Id FROM MARKS_TABLE
RENAME Student_Stud_Id TO Stud_Id FROM FINAL_TABLE
PRINT FINAL_TABLE
